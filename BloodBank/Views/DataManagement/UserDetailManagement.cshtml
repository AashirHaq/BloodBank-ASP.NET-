@model List<BloodBank.Models.UserData>
@{
    ViewBag.Title = "UserDetailManagement";
}

<h2>UserDetailManagement</h2>

<div>
    <table class="table">
        <thead>
            <tr>
                <th>ID</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Username</th>
                <th>Email Address</th>
                <th>Password</th>
                <th>Controller</th>
            </tr>
        </thead>
        @{
            foreach (var s in Model)
            {
                <tbody>
                    <tr>
                        <td>@s.Id</td>
                        <td>@s.FirstName</td>
                        <td>@s.LastName</td>
                        <td>@s.Username</td>
                        <td>@s.EmailAddress</td>
                        <td>@s.Password</td>
                        <td>@Html.ActionLink("Update", "UpdateUserDetail", "DataManagement", new { Id = @s.Id }, null)</td>
                    </tr>
                </tbody>

            }
        }
    </table>
</div>
