@model List<BloodBank.Models.DonorData>
@{
    ViewBag.Title = "DonorDataManagement";
}

<h2>DonorDataManagement</h2>

<div>
    <table class="table">
        <thead>
            <tr>
                <th>ID</th>
                <th>Name</th>
                <th>Gender</th>
                <th>Address</th>
                <th>Contact Number</th>
                <th>Blood Group</th>
                <th>date Of Birth</th>
                <th>Controller</th>
            </tr>
        </thead>
        @{
            foreach (var s in Model)
            {
                <tbody>
                    <tr>
                        <td>@s.DonorID</td>
                        <td>@s.FullName</td>
                        <td>@s.Gender</td>
                        <td>@s.Address</td>
                        <td>@s.ContactNumber</td>
                        <td>@s.BloodGroup</td>
                        <td>@s.DateOfBirth</td>
                        <td>@Html.ActionLink("Update", "UpdateDonorData", "DataManagement", new { Id = @s.DonorID }, null)</td>
                    </tr>
                </tbody>

            }
        }

    </table>
</div>
